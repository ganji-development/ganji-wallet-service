NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Solana Testnet
SOLANA_TESTNET_RPC_URL=https://api.devnet.solana.com
SOLANA_TESTNET_WALLET_PATH=./secrets/master-keypair-testnet.json
GANJI_TESTNET_TOKEN_MINT=your-testnet-token-mint-address-here
SOLANA_TESTNET_LICENSE_PROGRAM_ID=your-testnet-license-program-id-here

# Solana Mainnet
SOLANA_MAINNET_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_MAINNET_WALLET_PATH=./secrets/master-keypair-mainnet.json
GANJI_MAINNET_TOKEN_MINT=your-mainnet-token-mint-address-here
SOLANA_MAINNET_LICENSE_PROGRAM_ID=your-mainnet-license-program-id-here

# Litecoin Testnet
LITECOIN_TESTNET_RPC_URL=http://localhost:9332
LITECOIN_TESTNET_RPC_USER=user
LITECOIN_TESTNET_RPC_PASS=password

# Litecoin Mainnet
LITECOIN_MAINNET_RPC_URL=http://localhost:19332
LITECOIN_MAINNET_RPC_USER=user
LITECOIN_MAINNET_RPC_PASS=password

# Auth
AUTH_API_KEY=development-api-key
